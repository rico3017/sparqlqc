PREFIX : <http://example.org/>







SELECT * WHERE 
{ 
  ?x a :Student .  
  {?x :name ?y } 
    UNION 
  {?x :nickName ?z } 
  UNION 
  {?x :telephone ?tel } 
  UNION 
  {?x :ssn ?ssn } 
     UNION 
  {?x :sex ?sex }
   UNION 
  {?x :memberOf ?dept } 
 UNION 
  {?x :emailAddress ?email }
  UNION 
  {?x :age ?age }  
 ?x :takesCourse ?course .
}
